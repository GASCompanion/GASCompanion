<!doctype html>
<html lang="en" data-color-mode="dark" data-dark-theme="dark_dimmed">

    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="{{ '/_static/css/primer.css' | url }}">
        <link rel="stylesheet" href="{{ '/_static/css/prism.css' | url }}">
        <style>
            .markdown-content {
                box-sizing: border-box;
                margin: 0 auto;
            }

            .notes {
                background: #ffedd5;
                color: #292524;
                padding: 16px 32px;
                border: 4px solid #fdba74;
            }

            .notes ul {
            }

            .markdown-body .language-cpp code br {
                display: block;
            }

            @media (max-width: 767px) {
                .markdown-body {
                    padding: 15px;
                }
            }
        </style>
        <title>{{ title }}</title>
    </head>

    <body>
        {{ content | safe }}
    </body>

</html>