	<category>Add Event|GSC|Character</category>
	<inputs />
	<outputs>
		<param>
			<name>Output Delegate</name>
			<type>Delegate</type>
			<description></description>
		</param>
		<param>
			<name>Out</name>
			<type>Exec</type>
			<description></description>
		</param>
		<param>
			<name>Delta Value</name>
			<type>Float</type>
			<description>Change in stamina value, positive for heal, negative for cost. If 0 the delta is unknown</description>
		</param>
		<param>
			<name>Event Tags</name>
			<type>Gameplay Tag Container Structure (by ref)</type>
			<description>The gameplay tags of the event that changed mana</description>
		</param>
	</outputs>
